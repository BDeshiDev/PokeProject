<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.networking.NetworkController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="playerNameField" prefHeight="25.0" prefWidth="324.0" promptText="&quot;enter player name&quot;" />
               </children>
            </HBox>
            <Pane prefHeight="57.0" prefWidth="600.0">
               <children>
                  <Label layoutX="14.0" text="Select party">
                     <font>
                        <Font size="27.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <FlowPane hgap="10.0" prefHeight="91.0" prefWidth="560.0" vgap="10.0">
               <children>
                  <ComboBox fx:id="SlotCheckBox1" prefWidth="150.0" promptText="Pick pokemon 1" />
                  <ComboBox fx:id="SlotCheckBox2" layoutX="30.0" layoutY="30.0" prefWidth="150.0" promptText="Pick pokemon 2" />
                  <ComboBox fx:id="SlotCheckBox3" layoutX="190.0" layoutY="30.0" prefWidth="150.0" promptText="Pick pokemon 3" />
                  <ComboBox fx:id="SlotCheckBox4" layoutX="350.0" layoutY="30.0" prefWidth="150.0" promptText="Pick pokemon 4" />
                  <ComboBox fx:id="SlotCheckBox5" layoutX="30.0" layoutY="65.0" prefWidth="150.0" promptText="Pick pokemon 5" />
                  <ComboBox fx:id="SlotCheckBox6" layoutX="190.0" layoutY="65.0" prefWidth="150.0" promptText="Pick pokemon 6" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </FlowPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#findHost" prefHeight="61.0" prefWidth="180.0" text="Start" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
